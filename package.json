{
  "name": "@exakt/ui",
  "version": "0.0.19",
  "description": "A UI library for Nuxt.js",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@nuxt/kit": "^3.2.3",
    "blurhash": "^2.0.5",
    "lodash-es": "^4.17.21",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.3",
    "@nuxt/test-utils": "^3.2.3",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.2.4",
    "changelogen": "^0.5.1",
    "eslint": "^8.35.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "mkdist": "^1.2.0",
    "nuxt": "^3.7.1",
    "vitest": "^0.29.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wd-4000/exakt.git"
  },
  "keywords": [
    "UI",
    "components",
    "Nuxt",
    "CSS",
    "epic"
  ],
  "author": "wd-4000",
  "bugs": {
    "url": "https://github.com/wd-4000/exakt/issues"
  },
  "resolutions": {
    "mkdist": "npm:@wd4000/mkdist-no-scss@1.2.0"
  },
  "homepage": "https://github.com/wd-4000/exakt#readme",
  "packageManager": "yarn@3.5.1"
}